{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
	"eslint:recommended",
	"plugin:vue/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2018,
	"sourceType": "module"
    },
    "parser": "vue-eslint-parser",
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
	"no-console": "off"
    }
}
